{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "@builder.io/qwik",
    "resolveJsonModule": true,
    "noImplicitAny": false,
  },
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "kaplay",
    "scripts",
    "dist",
    "public"
  ]
}